<p id="notice"><%= notice %></p>

<p>
  <strong>Start:</strong>
  <%= @prediction.start %>
</p>

<p>
  <strong>End:</strong>
  <%= @prediction.end %>
</p>

<p>
  <strong>Location:</strong>
  <%= @prediction.location.name %>
</p>

<p>
  <strong>Issuing User:</strong>
  <%= @prediction.user.email %>
</p>

<p>
  <% if @prediction.report %>
  	  <strong>Confirming Report: </strong>
	  <%= @prediction.report.start%>
	  <%= @prediction.report.end %>
	  <%= @prediction.report.user.email %>
  <% end %>
</p>
<%= link_to 'Edit', edit_prediction_path(@prediction) %> |
